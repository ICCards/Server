"language": "GDScript",
"path": "res://assets/maujoe.custom_gradient_texture/custom_gradient_texture.gd"
}, {
"base": "Node",
"class": "Message",
"language": "GDScript",
"path": "res://models/Message.gd"
}, {
"base": "AnimationPlayer",
